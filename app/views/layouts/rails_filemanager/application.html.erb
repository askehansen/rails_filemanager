<!DOCTYPE html>
<html>
<head>
  <title>Filemanager</title>
  <%= stylesheet_link_tag    "rails_filemanager/application.css", :media => "all" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= yield %>

<script type="text/javascript">
	var SessionParams = {
		"<%= key = Rails.application.config.session_options[:key] %>": "<%= cookies[key] %>",
		"<%= request_forgery_protection_token %>": "<%= form_authenticity_token %>"
	};
</script>

<%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" %>
<%= javascript_include_tag "rails_filemanager/application.js" %>
</body>
</html>
